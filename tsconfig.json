{
    "compilerOptions": {
        "target": "ES2022",
        "module": "node16",
        "lib": ["ES2022"],
        "declaration": true,
        "sourceMap": true,
        "outDir": "dist",
        "rootDir": "src",
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "noUncheckedIndexedAccess": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}
