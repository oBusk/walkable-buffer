{
    "name": "walkable-buffer",
    "version": "3.0.2",
    "description": "üö∂üõ°Ô∏è A class for easily reading data from binary Buffers",
    "keywords": [
        "binary",
        "buffer",
        "walk"
    ],
    "author": "Oscar Busk <oscar.busk@gmail.com>",
    "license": "MIT",
    "homepage": "https://github.com/oBusk/walkable-buffer#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/oBusk/walkable-buffer.git"
    },
    "bugs": {
        "url": "https://github.com/oBusk/walkable-buffer/issues"
    },
    "engines": {
        "node": "~10.18.1 || ^10.20.0 || ^12.16.0 || >=14.2.0"
    },
    "volta": {
        "node": "14.4.0"
    },
    "type": "commonjs",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "module": "dist.module/index.js",
    "sideEffects": false,
    "bundlewatch": {
        "files": [
            {
                "path": "dist?(.module)/WalkableBuffer.js",
                "maxSize": "3.5kB"
            },
            {
                "path": "dist?(.module)/!(WalkableBuffer|*.spec).js",
                "maxSize": "0.4kB"
            }
        ]
    },
    "scripts": {
        "version": "npm run build",
        "build": "rimraf dist/ dist.module/ && tsc && tsc -P tsconfig.module.json",
        "lint": "eslint **/*.{ts,js,json} --ignore-path .gitignore",
        "lint-fix": "eslint **/*.{ts,js,json} --ignore-path .gitignore --fix",
        "prettier": "prettier **/*.{md,yml,yaml} --ignore-path .gitignore --check",
        "prettier-fix": "prettier **/*.{md,yml,yaml} --ignore-path .gitignore --write",
        "postlint": "npm run prettier",
        "postlint-fix": "npm run prettier-fix",
        "test": "jest",
        "test-coverage": "jest --coverage",
        "bundlewatch": "bundlewatch"
    },
    "dependencies": {
        "read-bigint": "^0.1.2"
    },
    "devDependencies": {
        "@types/jest": "^26.0.0",
        "@types/node": "^10.17.26",
        "@typescript-eslint/eslint-plugin": "^3.2.0",
        "@typescript-eslint/parser": "^3.2.0",
        "bundlewatch": "^0.2.7",
        "eslint": "^7.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-jest": "^23.16.0",
        "eslint-plugin-prettier": "^3.1.4",
        "husky": "^4.2.5",
        "jest": "^26.0.1",
        "lint-staged": "^10.2.11",
        "prettier": "^2.0.5",
        "rimraf": "^3.0.2",
        "ts-jest": "^26.1.0",
        "typescript": "^3.9.5"
    }
}
