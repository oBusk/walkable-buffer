{
    "author": "Oscar Busk <oscar.busk@gmail.com>",
    "name": "walkable-buffer",
    "version": "2.0.8",
    "description": "🚶🛡️ A class for easily reading data from binary Buffers",
    "license": "MIT",
    "repository": "git+https://github.com/oBusk/walkable-buffer.git",
    "scripts": {
        "build": "pika build",
        "deploy": "pika publish",
        "jest": "jest",
        "test": "jest",
        "posttest": "npm run typecheck && npm run lint",
        "lint": "tslint --project tsconfig.json --format stylish",
        "typecheck": "tsc --noEmit",
        "test:watch": "jest --watch --updateSnapshot",
        "version": "npm run build && npm run docs && git add api && git add api -u",
        "docs": "typedoc --out api --mode file --excludeExternals --excludeNotExported --excludePrivate --excludeProtected --tsconfig src/tsconfig.app.json --theme markdown --readme none"
    },
    "keywords": [
        "binary",
        "buffer",
        "walk"
    ],
    "engines": {
        "node": "~10.15.3 || ^10.18.0 || ^12.14.0 || >=13.6.0"
    },
    "volta": {
        "node": "10.18.0"
    },
    "dependencies": {
        "@types/node": "^10.15.0",
        "tslib": "^1.10.0"
    },
    "devDependencies": {
        "@pika/pack": "^0.5.0",
        "@pika/plugin-build-node": "^0.8.1",
        "@pika/plugin-build-web": "^0.8.1",
        "@pika/plugin-bundle-web": "^0.8.1",
        "@pika/plugin-ts-standard-pkg": "^0.8.1",
        "@types/jest": "^24.0.25",
        "jest": "^25.1.0",
        "ts-jest": "^25.0.0",
        "tslint": "^6.0.0",
        "typedoc": "^0.16.3",
        "typedoc-plugin-markdown": "^2.2.16",
        "typescript": "^3.7.4",
        "typescript-tslint-plugin": "^0.5.5"
    },
    "@pika/pack": {
        "pipeline": [
            [
                "@pika/plugin-ts-standard-pkg",
                {
                    "tsconfig": "src/tsconfig.app.json"
                }
            ],
            [
                "@pika/plugin-build-node",
                {
                    "minNodeVersion": "10.15.3"
                }
            ],
            [
                "@pika/plugin-build-web",
                {
                    "entrypoint": "module"
                }
            ],
            [
                "@pika/plugin-bundle-web",
                {
                    "entrypoint": "browser"
                }
            ]
        ]
    }
}
