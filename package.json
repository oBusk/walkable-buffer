{
    "author": "Oscar Busk <oscar.busk@gmail.com>",
    "name": "walkable-buffer",
    "version": "1.2.2",
    "description": "A class for easily reading data from binary Buffers.",
    "license": "MIT",
    "repository": "git+https://github.com/oBusk/walkable-buffer.git",
    "main": "lib/index.js",
    "module": "es/index.js",
    "types": "es/index.d.ts",
    "scripts": {
        "build": "npm run build:lib & npm run build:es",
        "build:lib": "tsc --outDir lib --module commonjs",
        "build:es": "tsc --outDir es --module es2015",
        "clean": "rimraf es lib coverage",
        "typecheck": "tsc --noEmit",
        "lint": "tslint --project tsconfig.json --format stylish",
        "test": "jest --env=jsdom --coverage",
        "test:watch": "jest --env=jsdom --watch --updateSnapshot",
        "prepublishOnly": "npm run build",
        "prebuild": "npm run clean",
        "postbuild": "rimraf {lib,es}/**/__tests__ {lib,es}/**/*.{spec,test}.{js,d.ts,js.map}",
        "posttest": "npm run typecheck && npm run lint",
        "preversion": "npm test && npm run build",
        "postversion": "git push && git push --tags"
    },
    "keywords": [
        "binary",
        "buffer",
        "walk"
    ],
    "files": [
        "lib",
        "es"
    ],
    "engines": {
        "node": ">=6.5.0"
    },
    "dependencies": {
        "@types/node": "^8.0.0",
        "int64-buffer": "^0.99.1007",
        "tslib": "^1.9.3"
    },
    "devDependencies": {
        "@types/jest": "^23.3.2",
        "jest": "^23.6.0",
        "rimraf": "^2.6.2",
        "ts-jest": "^23.10.1",
        "tslint": "^5.11.0",
        "typescript": "^3.0.3"
    }
}
