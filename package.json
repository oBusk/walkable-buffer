{
    "author": "Oscar Busk <oscar.busk@gmail.com>",
    "name": "walkable-buffer",
    "version": "1.2.6",
    "description": "ğŸš¶ğŸ›¡ï¸ A class for easily reading data from binary Buffers",
    "license": "MIT",
    "repository": "git+https://github.com/oBusk/walkable-buffer.git",
    "main": "lib/index.js",
    "module": "es/index.js",
    "types": "es/index.d.ts",
    "scripts": {
        "build": "npm run build:lib & npm run build:es",
        "build:lib": "tsc --p src/tsconfig.app.json --outDir lib --module commonjs",
        "build:es": "tsc --p src/tsconfig.app.json --outDir es --module es2015",
        "clean": "rimraf tsc-out es lib coverage",
        "typecheck": "tsc --noEmit",
        "lint": "tslint --project tsconfig.json --format stylish",
        "test": "jest --coverage",
        "test:watch": "jest --watch --updateSnapshot",
        "prepublishOnly": "npm run build",
        "prebuild": "npm run clean",
        "posttest": "npm run typecheck && npm run lint",
        "preversion": "npm test && npm run build",
        "postversion": "git push && git push --tags"
    },
    "keywords": [
        "binary",
        "buffer",
        "walk"
    ],
    "files": [
        "lib",
        "es"
    ],
    "engines": {
        "node": ">=6.14.0"
    },
    "dependencies": {
        "@types/node": "^6.14.0",
        "int64-buffer": "~0.99.0",
        "tslib": "^1.10.0"
    },
    "devDependencies": {
        "@types/jest": "^24.0.15",
        "jest": "^24.8.0",
        "rimraf": "^2.6.3",
        "ts-jest": "^24.0.2",
        "tslint": "^5.18.0",
        "typescript": "^3.5.2",
        "typescript-tslint-plugin": "^0.5.3"
    }
}
