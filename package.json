{
    "name": "walkable-buffer",
    "version": "3.0.0-0",
    "description": "🚶🛡️ A class for easily reading data from binary Buffers",
    "keywords": [
        "binary",
        "buffer",
        "walk"
    ],
    "author": "Oscar Busk <oscar.busk@gmail.com>",
    "license": "MIT",
    "repository": "git+https://github.com/oBusk/walkable-buffer.git",
    "sideEffects": false,
    "scripts": {
        "build": "rimraf dist/ dist.module/ && tsc -p tsconfig.app.json && tsc -P tsconfig.module.app.json",
        "jest": "jest",
        "lint": "tslint --project tsconfig.json --format stylish",
        "posttest": "npm run typecheck && npm run lint",
        "test": "jest",
        "test:watch": "jest --watch --updateSnapshot",
        "typecheck": "tsc --noEmit",
        "version": "npm run build"
    },
    "engines": {
        "node": "~10.15.3 || ^10.18.0 || ^12.14.0 || >=13.6.0"
    },
    "volta": {
        "node": "13.7.0"
    },
    "dependencies": {
        "@types/node": "^10.15.0",
        "tslib": "^1.11.0"
    },
    "devDependencies": {
        "@types/jest": "^25.1.3",
        "jest": "^25.1.0",
        "rimraf": "^3.0.2",
        "ts-jest": "^25.2.1",
        "tslint": "^6.0.0",
        "typescript": "^3.8.2",
        "typescript-tslint-plugin": "^0.5.5"
    },
    "main": "dist/index.js",
    "module": "dist.module/index.js",
    "typings": "dist/index.d.ts"
}
