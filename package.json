{
    "author": "Oscar Busk <oscar.busk@gmail.com>",
    "name": "walkable-buffer",
    "version": "1.2.7",
    "description": "🚶🛡️ A class for easily reading data from binary Buffers",
    "license": "MIT",
    "repository": "git+https://github.com/oBusk/walkable-buffer.git",
    "scripts": {
        "build": "pack build",
        "deploy": "pack publish",
        "jest": "jest",
        "test": "jest",
        "posttest": "npm run typecheck && npm run lint",
        "lint": "tslint --project tsconfig.json --format stylish",
        "typecheck": "tsc --noEmit",
        "test:watch": "jest --watch --updateSnapshot",
        "version": "npm run build"
    },
    "keywords": [
        "binary",
        "buffer",
        "walk"
    ],
    "engines": {
        "node": ">=10.4.0"
    },
    "dependencies": {
        "@types/node": "^10.4.0",
        "tslib": "^1.10.0"
    },
    "devDependencies": {
        "@pika/pack": "^0.4.0",
        "@pika/plugin-build-node": "^0.4.0",
        "@pika/plugin-build-types": "^0.4.0",
        "@pika/plugin-build-web": "^0.4.0",
        "@pika/plugin-ts-standard-pkg": "^0.4.0",
        "@types/jest": "^24.0.15",
        "jest": "^24.8.0",
        "ts-jest": "^24.0.2",
        "tslint": "^5.18.0",
        "typescript": "^3.5.3",
        "typescript-tslint-plugin": "^0.5.4"
    },
    "@pika/pack": {
        "pipeline": [
            [
                "@pika/plugin-ts-standard-pkg",
                {
                    "tsconfig": "src/tsconfig.app.json"
                }
            ],
            [
                "@pika/plugin-build-node",
                {
                    "minNodeVersion": 10
                }
            ],
            [
                "@pika/plugin-build-web"
            ]
        ]
    }
}
